{
  "name": "ha_v1_be",
  "version": "1.0.0",
  "description": "Back end for HA V1",
  "main": "./src/index.ts",
  "scripts": {
    "start:in-memory": "set HA_APP_DATA_SOURCE=IN_MEMORY&& nodemon --legacy-watch ./src/index.ts",
    "sim": "npm run start:in-memory",
    "start:remote": "set HA_APP_DATA_SOURCE=FIREBASE&& nodemon --legacy-watch ./src/index.ts",
    "sr": "npm run start:remote",
    "build": "tsc",
    "clean": "npm run prettier:fix && npm run lint:fix",
    "lint:fix": "eslint src --ext .ts --fix",
    "prettier:fix": "npx prettier ./src --write",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "author": "DasminX",
  "license": "ISC",
  "dependencies": {
    "@dasminx/hang-around-common": "^1.0.14",
    "@googlemaps/places": "1.4.0",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.1",
    "express": "^4.19.2",
    "express-rate-limit": "7.1.5",
    "firebase": "^10.12.2",
    "firebase-admin": "^12.2.0",
    "helmet": "7.1.0",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "nodemon": "3.0.3",
    "redis": "^4.6.15",
    "ts-node": "10.9.2",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "vitest": "^2.0.5"
  }
}